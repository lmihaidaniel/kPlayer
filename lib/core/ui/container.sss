.kmlContainers{
	position: absolute;
	overflow: hidden;
	pointer-events: none;
}
.kmlWidget{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  pointer-events: all;
  opacity: 1;
  transition: opacity .25s ease;
  transition: opacity 0.25s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}

.kmlWidget.hidden{
	opacity: 0!important;
}
