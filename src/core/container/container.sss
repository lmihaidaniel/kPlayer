.kmlContainers{
	position: absolute;
	overflow: hidden;
	pointer-events: none;
}
.kmlContainer{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.kmlContainer .body{
	position: absolute;
	pointer-events: all;
	opacity: 1;
	transition: opacity .25s ease;
  transition: opacity 0.25s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}

.kmlContainer.hidden .body{
	opacity: 0!important;
}
