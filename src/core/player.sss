@import 'reset-css/reset.css';

.kmlPlayer{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	overflow: hidden;
}

.kmlPlayer.disableSelect{
	user-select: none;
}

.kmlPlayer:fullscreen, .kmlPlayer.inFrame {
	position: absolute !important;
	top: 0 !important;
	left: 0 !important;
	width: 100% !important;
	height: 100% !important;
	max-width: 100% !important;
	max-height: 100% !important;
	margin: 0 !important;
	transform: none!important;
}