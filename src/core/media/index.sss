.kmlVideo {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: black;
  &:focus{
    outline: none;
  }
  &.fluid{
  	top: 50%;
  	left: 50%;
  	width: auto;
  	height: auto;
  	transform: translate(-50%, -50%);
    &.width{
      width: 100%;
    }
    &.height{
      height: 100%;
    }
  }
}